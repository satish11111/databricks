create table databricks.alldatatypes(
   big_int_col1 BIGINT,
   boolean_col BOOLEAN,
   date_col3 DATE,
   decimal_col4 decimal(4,1),
   double_col5 double,
   float_col6 float,
   int_col7 int,
   small_int_col8 smallInt,
   string_col9 STRING,
   time_stamp_col10 TIMESTAMP

);



insert into databricks.alldatatypes values(101,true,'2022-01-20 17:31:20',200.1,3.22,4.33,101,1,'satish','2022-01-21 00:00:00');





select * from databricks.alldatatypes;
